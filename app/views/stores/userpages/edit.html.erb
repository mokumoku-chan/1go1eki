<%= render 'layouts/title', title: "編集ページ" %>


<%= form_with model: @store, url: stores_mypage_update_path, local: true do |f| %>
  <%= f.label :name, "店名" %>
  <%= f.text_field :name %>

  <%= f.label :email, "メールアドレス" %>
  <%= f.text_field :email %>


  <%= f.label :telephone_number, "電話番号" %>
  <%= f.text_field :telephone_number %>

  <%= f.label :postal_code, "郵便番号" %>
  <%= f.text_field :postal_code %>

  <%= f.label :address, "住所" %>
  <%= f.text_field :address %>


  <%= f.submit "登録内容更新" %>


<% end %>