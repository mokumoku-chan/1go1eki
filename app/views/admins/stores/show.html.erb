<h2>ストアユーザ</h2>


<table border="1">
  <tr>
    <th>No.</th>
    <td><%= @store.id %></td>
  </tr>

  <tr>
    <th>ストア名</th>
    <td><%= @store.name %></td>
  </tr>

  <tr>
    <th>メールアドレス</th>
    <td><%= @store.email %></td>
  </tr>

  <tr>
    <th>電話番号</th>
    <td><%= @store.telephone_number %></td>
  </tr>

  <tr>
    <th>住所</th>
    <td>
      〒<%= @store.postal_code %>
      <%= @store.address %>
    </td>
  </tr>

  <tr>
    <th>ステータス</th>
    <td>
      <% if @store.is_active? %>
        有効
      <% else %>
        退会済み
      <% end %>
    </td>
  </tr>
</table>


<%= link_to "編集", edit_admins_store_path(@store.id) %>