<h2>編集</h2>

<%= form_with model: @user, url: admins_user_path(@user.id), local:true do |f| %>

  <%= f.text_field :name %>
  <%= f.text_field :email %>
  <%= f.text_field :telephone_number %>
  <%= f.label :is_active, "有効" %>
  <%= f.radio_button :is_active, true %>
  <%= f.label :is_active, "退会済み" %>
  <%= f.radio_button :is_active, false %>

  <%= f.submit "変更" %>

<% end %>

